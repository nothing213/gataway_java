<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.wl.dao.ServerDao">

    <select id="getServerMsg" resultType="ServerBean">
        select * from server_forecast;
    </select>

     <insert id="setServerMsg" parameterType="ServerBean" useGeneratedKeys="true" keyProperty="ServerID">
        insert into
        server(ServerName,ServerHost,ServerPort,ServerLB)
        values(#{ServerName},#{ServerHost},#{ServerPort},#{ServerLB});
     </insert>
</mapper>
